{
    "name": "sai/drupal-releases",
    "description": "Client for http://updates.drupal.org/release-history/",
    "keywords": ["drupal"],
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Shawn Iwinski",
            "email": "shawn.iwinski@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "guzzle/http": "~3.4",
        "symfony/console": "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-0": {
            "SAI\\DrupalReleases": "src/",
            "SAI\\DrupalReleases\\Tests": "tests/"
        }
    },
    "bin": ["bin/drupal-releases"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
