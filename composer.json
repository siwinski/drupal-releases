{
    "name": "sai/drupal-releases",
    "description": "Client for http://updates.drupal.org/release-history/",
    "license": "GPLv2+",
    "authors": [
        {
            "name": "Shawn Iwinski",
            "email": "shawn.iwinski@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "guzzle/http": "~3.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "SAI\\DrupalReleases": "src/",
            "SAI\\DrupalReleases\\Tests": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
